		"@graphql-mesh/transform-prefix": "^0.102.12",
		"@graphql-mesh/transform-rename": "^0.102.12",
		"@graphql-mesh/transform-type-merging": "^0.102.12",
		"arctic": "^2.2.2",
		"dotenv": "^16.4.5",
		"graphql": "^16.9.0",
